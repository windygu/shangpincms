                if (i==1)
                {
                    if (Model.Count(p => p.RegionID == i) == 1) 
                    {
                        obj = Model.Single(p => p.RegionID == i);
                        <li opratorRegion="@i" relationtype="@obj.RelationType">
                            <a id="big" href="@obj.RelationContent" navtitle="国际大牌" name="big" rnav="ranv" target="_blank"></a>
                                <img  lazy="@(Shangpin.Framework.WebMvc.ServicePic.ResolveUGCImage("2", obj.ImgNO, 231, 365))" width="231" height="365">
                                <span class="hover_bg" style="display: none;"></span>
                        </li>
                    }
                    else
                    {
                        <li opratorRegion="@i" relationtype="3">
                            <a id="big" href="#" navtitle="国际大牌" name="big" rnav="ranv" target="_blank"></a>
                                <img width="231" height="365" />
                                <span class="hover_bg" style="display: none;"></span>
                        </li>
                    }
                }
