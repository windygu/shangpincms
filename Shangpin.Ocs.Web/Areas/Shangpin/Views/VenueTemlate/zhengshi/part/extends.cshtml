                                                @Html.Raw("全场￥<b>" + obj.PriceName + "</b>")
                                           break;
                                                case 10:
                                                @Html.Raw("全场<b>" + obj.PriceName + "</b>折")
                                           break;
                                                case 11:
                                                @Html.Raw("满减￥<b>" + obj.PriceName + "</b>")
                                           break;
                                                case 12:
                                                @Html.Raw("全场狂欢")
