    <script type="text/javascript" src="/Areas/Outlet/Scripts/plug/comm.js"></script>
    <script type="text/javascript">
        (function (window, $, undefined) {
            $(function () {
                //UpdateProductMarketPrice(); //更新商品市场价
            });
        })(window, jQuery);

        //更新商品市场价
        function UpdateProductMarketPrice() {
            var arrayProductNo = '';
            $("input[name='hidProductNo']").each(function () {
                arrayProductNo = arrayProductNo + "," + $(this).val();
            });
            if (arrayProductNo != "") {
                $.ajax({
                    url: "/Outlet/Subject/AjaxUpdateProductMarketPrice",
                    type: "post",
                    dataType: "json",
                    data: "productNos=" + arrayProductNo + "&t=" + new Date().getTime(),
                    success: function (data) {
                        if (data != null) {
                            for (var i = 0; i < data.jsonProduct.length; i++) {
                                $("#productMarketPrice_" + data.jsonProduct[i].ProductNo).html(data.jsonProduct[i].MarketPrice);
                                //alert(data.jsonProduct[i].ProductNo);
                                //alert(data.jsonProduct[i].MarketPrice);
                                //alert($("#productMarketPrice_" + data.jsonProduct[i].ProductNo).val());
                            }
                        }
                    },
                    error: function (x, e) {
                        alert(x.responseText);
                    }
                });
            }

        }
    </script>
}
